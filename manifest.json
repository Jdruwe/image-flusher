{
  "name": "A.S.Adventure Image Flusher",
  "version": "1.2",
  "description": "Flushes images using the Jenkins job.",
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "http://jenkins.aws.intranet/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "assets/images/icon16.png",
      "32": "assets/images/icon32.png",
      "48": "assets/images/icon48.png",
      "128": "assets/images/icon128.png"
    }
  },
  "icons": {
    "16": "assets/images/icon16.png",
    "32": "assets/images/icon32.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  },
  "web_accessible_resources": [
    "*.mp3"
  ],
  "options_page": "options.html",
  "manifest_version": 2
}